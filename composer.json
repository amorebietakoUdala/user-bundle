{
    "name": "amorebietakoudala/user-bundle",
    "description": "AmorebietakoUdala erabiltzaile bundlela",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Amorebietako Udala",
            "email": "informatika@amorebieta.eus"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "doctrine/orm": "^2.16|^3.0",
        "symfony/framework-bundle": "6.*|7.*",
        "symfony/http-foundation": "6.*|7.*",
        "symfony/security-bundle": "6.*|7.*",
        "symfony/ldap": "6.*|7.*",
        "symfony/form": "6.*|7.*",
        "twig/twig": "^3.4",
        "doctrine/doctrine-bundle": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "AMREU\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AMREU\\UserBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/browser-kit": "6.*|7.*",
        "symfony/css-selector": "6.*|7.*",
        "symfony/phpunit-bridge": "6.*|7.*"
    }
}
