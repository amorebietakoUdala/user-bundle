{
    "name": "amorebietakoudala/user-bundle",
    "description": "AmorebietakoUdala erabiltzaile bundlela",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Amorebietako Udala",
            "email": "informatika@amorebieta.eus"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.16",
        "symfony/framework-bundle": "5.*|6.*",
        "symfony/security-bundle": "5.*|6.*",
        "symfony/ldap": "5.*|6.*",
        "symfony/form": "5.*|6.*",
        "twig/twig": "^3.4",
        "doctrine/doctrine-bundle": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "AMREU\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AMREU\\UserBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/browser-kit": "^5.4|6.*",
        "symfony/css-selector": "^5.4|6.*",
        "symfony/phpunit-bridge": "^5.4|6.*"
    }
}
