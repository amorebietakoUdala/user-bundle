{
    "name": "amorebietakoudala/user-bundle",
    "description": "AmorebietakoUdala erabiltzaile bundlela",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Amorebietako Udala",
            "email": "informatika@amorebieta.eus"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "symfony/framework-bundle": "^4.4||5.*",
        "symfony/security-bundle": "^5.3",
        "symfony/ldap": "^4.4||5.*",
        "symfony/form": "^4.4||5.*",
        "doctrine/doctrine-bundle": "^2.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AMREU\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AMREU\\UserBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/browser-kit": "^4.4||^5.1",
        "symfony/css-selector": "^4.4||^5.1",
        "symfony/phpunit-bridge": "^4.4||^5.1"
    }
}
